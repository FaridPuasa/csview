<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MOH EXP ORDERS</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>
<body>
    

    <style>
        body {
          margin: 0;
          font-family: Arial, Helvetica, sans-serif;
        }
        
        .topnav {
          overflow: hidden;
          background-color: #333;
        }
        
        .topnav a {
          float: left;
          color: #f2f2f2;
          text-align: center;
          padding: 14px 16px;
          text-decoration: none;
          font-size: 17px;
        }
        
        .topnav a:hover {
          background-color: #ddd;
          color: black;
        }
        
        .topnav a.active {
          background-color: #04AA6D;
          color: white;
        }
        </style>
   <div class="topnav">
    <a href="/StdMOH">MOH STD ORDERS</a>
   <a href="/ExpMOH">MOH EXP ORDERS</a>
    <a href="/stdjpmc">JPMC STD ORDERS</a>
  <a href="/stdphc">PHC STD ORDERS</a>
    <a href="/ImeMoh">MOH IME ORDERS</a>
   <a href="/">USERS</a>
   <a href="/guestmoh">GUEST MOH</a>
   <a href="/expjpmc">JPMC EXP ORDERS</a>

 

         
       <input type="text" id="mohexpInput" onkeyup="searchTable()" placeholder="Search">
    
      </div>

    <table id="mohexpTable" class="table table-light table-striped table-bordered table-hover">
      <tbody>
        <tr>
      <th scope="col">Quarantine</th>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
      <th scope="col">Bruhims</th>
      <th scope="col">IC Number</th>
      <th scope="col">Appointment Place</th>
      <th scope="col">Phone Number</th>
      <th scope="col">Additional Phone Number</th>
      <th scope="col">Type of Delivery</th>
      <th scope="col">Remarks</th>
      <th scope="col">Payment</th>
      <th scope="col">Submitted Date</th>
     <th scope="col">Email</th>
     <th scope="col">Health Center Districts</th>
     <th scope="col">Notification</th>
     <th scope="col">Duration</th>
     </tr>



        <tbody>
        <%exp_MohOrderList.forEach(exp_MohOrder => {%>
            <tr>
              <td><%= exp_MohOrder.qo %></td>
                <th scope="row"><%= exp_MohOrder.name %></th>
                <td><%= exp_MohOrder.kampong %>, <%= exp_MohOrder.jalan %>, <%= exp_MohOrder.simpang %>, <%= exp_MohOrder.house_Number %></td>             
                <td><%= exp_MohOrder.bruhims %></td>
                <td><%= exp_MohOrder.icNumber %></td>
                <td><%= exp_MohOrder.BNHC %>, <%= exp_MohOrder.TUHC %>, <%= exp_MohOrder.TEHC %>, <%= exp_MohOrder.BHC %></td>
                <td><%= exp_MohOrder.contact_1 %></td>
                <td><%= exp_MohOrder.contact_2 %></td>
                <td><%= exp_MohOrder.radioTOD %></td>
                <td><%= exp_MohOrder.re %></td>
                <td><%= exp_MohOrder.pm %></td>
                <td><%= exp_MohOrder.dateSubmit %></td>
                <td><%= exp_MohOrder.email %></td>
                <td><%= exp_MohOrder.radioDistrict %></td> 
                <td><%= exp_MohOrder.radioNOTI %></td>
                <td><%= exp_MohOrder.radioDURATION %></td>
            </tr>
        <%})%>
        </tbody>
      </table>
      <script>
        function searchTable() {
            var input, filter, found, table, tr, td, i, j;
            input = document.getElementById("mohexpInput");
            filter = input.value.toUpperCase();
            table = document.getElementById("mohexpTable");
            tr = table.getElementsByTagName("tr");
            for (i = 0; i < tr.length; i++) {
                td = tr[i].getElementsByTagName("td");
                for (j = 0; j < td.length; j++) {
                    if (td[j].innerHTML.toUpperCase().indexOf(filter) > -1) {
                        found = true;
                    }
                }
                if (found) {
                    tr[i].style.display = "";
                    found = false;
                } else {
                    tr[i].style.display = "none";
                }
            }
        }
        </script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

  

</body>
</html>
